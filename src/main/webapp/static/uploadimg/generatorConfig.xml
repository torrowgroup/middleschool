<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <classPathEntry
        location="/home/zjg/eclipse/middleschool/mysql-connector-java-5.1.30.jar" />
    <context id="mysql" targetRuntime="MyBatis3Simple"  defaultModelType="flat">
        <!-- 生成的Java文件的编码 -->
        <property name="javaFileEncoding" value="UTF-8"/>
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>  
        
        
                
        <commentGenerator>
        	<!-- 生成的代码不包含注释 -->
        	<property name="suppressAllComments" value="true"/>
        	<property name="suppressDate" value="true"/>
        </commentGenerator>
        
        
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
            connectionURL="jdbc:mysql://localhost:3306/school?useUnicode=true&amp;characterEncoding=UTF-8"
            userId="root" password="root" />
         
        <javaModelGenerator targetPackage="com.torrow.school.entity"
            targetProject="/home/zjg/eclipse/middleschool/src/main/java" >
       <!--    <property name="enableSubPackages" value="true" /> -->
          <property name="trimStrings" value="true" />
        </javaModelGenerator>
        
        
        <sqlMapGenerator targetPackage="mapping"
            targetProject="/home/zjg/eclipse/middleschool/src/main/resources" >
        </sqlMapGenerator>
        
        <javaClientGenerator targetPackage="com.torrow.school.dao"
            targetProject="/home/zjg/eclipse/middleschool/src/main/java" type="XMLMAPPER" />
   
        
        <!-- 生成全部的表 -->
        <table catalog="school"  tableName="%" >

        </table>
    </context>
</generatorConfiguration>